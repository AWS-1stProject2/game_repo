package com.care.boot.dto;

public class GameResult {
    private String playerName;   // 플레이어 닉네임
    private String playerMove;   // 플레이어가 선택한 값
    private String opponentName; // 상대방 닉네임 (서버 or 다른 유저)
    private String opponentMove; // 상대방이 선택한 값
    private String result;       // 승/패/무승부

    public GameResult(String playerName, String playerMove, String opponentName, String opponentMove, String result) {
        this.playerName = playerName;
        this.playerMove = playerMove;
        this.opponentName = opponentName;
        this.opponentMove = opponentMove;
        this.result = result;
    }

    public String getPlayerName() {
        return playerName;
    }

    public String getPlayerMove() {
        return playerMove;
    }

    public String getOpponentName() {
        return opponentName;
    }

    public String getOpponentMove() {
        return opponentMove;
    }

    public String getResult() {
        return result;
    }
}
